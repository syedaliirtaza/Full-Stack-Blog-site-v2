<%- include("partials/header.ejs")  -%>
<div class="home">
    <h1>Home</h1>
    <p><%= startingContent %></p>

    <% posts.forEach(function(post){ %>
        <h1><%= post.title %></h1>
    <p><%= post.content.substring(0, 100) + "...  " %>   
        <a style="color: red" href="/posts/<%= post.id %>">Read more</a>
    </p>
    <% }) %>
</div>    
<%- include("partials/footer.ejs"); -%>

